    "url": "https://github.com/campro2210/trello-clone-API/issues"
  },
  "homepage": "https://github.com/campro2210/trello-clone-API#readme",
  "dependencies": {
    "@babel/runtime": "^7.16.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "lodash": "^4.17.21",
    "mongodb": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
